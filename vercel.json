{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm run install:all",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/backend"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "outputDirectory": "packages/frontend/dist"
} 